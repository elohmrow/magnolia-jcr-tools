      <type>info.magnolia.jcrtools.JcrToolsView</type>
      <implementation>info.magnolia.jcrtools.JcrToolsViewImpl</implementation>
    </component>
  </components>

  <dependencies>
    <dependency>
      <name>core</name>
      <version>5.4.2/*</version>
    </dependency>
    <dependency>
      <name>ui-admincentral</name>
      <version>5.4.2/*</version>
